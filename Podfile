workspace 'MyiOS.xcworkspace'

target 'MyOC' do
  use_frameworks!
  platform :ios, '11.0'
  project 'MyOC/MyOC.xcodeproj'
  #  网络调试工具
  pod 'Bagel', '~>  1.3.2'
  #  3D 视角 iOS 应用布局视图检查器。
  pod 'LayoutInspector'
  
  pod 'Masonry', '~> 1.0.2'

  pod 'AFNetworking', '~> 3.2.1'
  

end

target 'MySwift' do
  use_frameworks!
  platform :ios, '11.0'
  project 'MySwift/MySwift.xcodeproj'
  pod 'Alamofire', '~> 5.0.0-rc.2'
  pod 'IBAnimatable'
  pod 'CircularCarousel'
  pod 'CardSlider'
#  pod 'Bagel', '~>  1.3.2'
end

target 'MySwiftUI' do
  platform :ios, '11.0'
  project 'MySwiftUI/MySwiftUI.xcodeproj'
  pod 'Alamofire', '~> 5.0.0-rc.2'
  
end
